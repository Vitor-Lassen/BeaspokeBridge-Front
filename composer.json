{
    "name": "bespoke-bridge/front",
    "description": "BeaspokeBridge Front-end with PHPMailer integration for Hostinger",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeaspokeBridge\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    }
}
